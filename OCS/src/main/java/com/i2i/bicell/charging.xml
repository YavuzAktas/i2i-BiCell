
<charging-rules>
    <zones>
        <zone>
            <service serviceName="voice">
                <regex>^5[0-5][0-9]{8}$</regex>
                <round>60</round>
                <price>0.50</price>
            </service>
            <service serviceName="sms" >
                <regex>^5[0-5][0-9]{8}$</regex>
                <round>1</round>
                <price>1</price>
            </service>
            <service serviceName="gb">
                <regex>[A-Z]</regex>
                <round>1</round>
                <price>1.5</price>
            </service>

        </zone>
    </zones>

</charging-rules>